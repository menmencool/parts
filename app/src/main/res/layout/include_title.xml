<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="70dp"
    android:background="@color/i_background"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_marginTop="20dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/include_banck"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_gravity="center"
            android:layout_marginLeft="17dp"
            android:src="@mipmap/back"
            android:visibility="gone" />

        <TextView
            android:id="@+id/include_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="26dp"
            android:text="云配在线"
            android:textColor="@color/i_white"
            android:textSize="16sp"
            android:visibility="gone" />

        <TextView
            android:id="@+id/include_titles"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="15dp"
            android:text="云配件gone"
            android:textColor="@color/i_white"
            android:textSize="16sp"
            android:visibility="gone" />

        <TextView
            android:id="@+id/include_title_zhong"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginRight="17dp"
            android:gravity="center"
            android:text="云配件gone"
            android:textColor="@color/i_white"
            android:textSize="16sp"
            android:visibility="gone" />

        <View
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/include_new"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="26dp"
            android:layout_marginRight="5dp"
            android:text="云配在线"
            android:textColor="@color/i_white"
            android:textSize="16sp"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/include_seek"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_gravity="center"
            android:layout_marginRight="17.5dp"
            android:src="@mipmap/sousuo"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/include_ivmes"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_gravity="center"
            android:layout_marginRight="5dp"
            android:src="@mipmap/sousuo"
            android:visibility="gone" />

        <TextView
            android:id="@+id/include_mes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginRight="5dp"
            android:text="云配在线"
            android:textColor="@color/i_white"
            android:textSize="16sp"
            android:visibility="gone" />
    </LinearLayout>
</LinearLayout>
